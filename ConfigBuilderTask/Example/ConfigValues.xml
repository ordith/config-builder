        <ConfigParameterValue Environment="Dev" Value="Example" />
        <ConfigParameterValue Environment="Stage" Value="Example" />
		<ConfigParameterValue Environment="Prod" Value="The Real Example" />
      </Values>
    </ConfigParameter>
  </Parameters>
</ConfigValues>